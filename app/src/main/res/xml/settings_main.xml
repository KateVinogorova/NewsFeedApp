<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <ListPreference
        android:defaultValue="@string/settings_sectioin_default"
        android:entries="@array/settings_section_labels"
        android:entryValues="@array/settings_section_values"
        android:key="@string/settings_section_key"
        android:title="@string/settings_section_label"></ListPreference>

    <SwitchPreference
        android:defaultValue="false"
        android:key="@string/settings_date_period_key"
        android:title="@string/settings_date_period_label"></SwitchPreference>

    <Preference
        android:key="@string/settings_choose_date_period_key"
        android:layout="@layout/layout"></Preference>

</PreferenceScreen>